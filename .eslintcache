 = confirmationResult;\n        Axios.post(`${BASE_URL}/forgot-password/`, {\n          mobile_number: mobile.mobile,\n        })\n          .then(history.replace({ pathname: \"/verify\", mobile: mobile }))\n          .catch((error) => {\n            setDisableSubmit(false);\n          });\n      })\n      .catch(function (error) {\n        console.log({ error });\n      });\n    setDisableSubmit(true);\n  };\n  return (\n    <Container component=\"main\" maxWidth=\"xs\">\n      <CssBaseline />\n      <div className={classes.paper}>\n        <Avatar className={classes.avatar}>\n          <LockOutlinedIcon />\n        </Avatar>\n        <Typography component=\"h1\" variant=\"h5\" fontWeight=\"bold\">\n          Verify mobile number\n        </Typography>\n        <form className={classes.form} noValidate>\n          <TextField\n            error={mobileFieldError}\n            helperText={mobileFieldError ? \"Invalid mobile number.\" : null}\n            variant=\"outlined\"\n            margin=\"normal\"\n            required\n            fullWidth\n            id=\"mobile\"\n            label=\"Mobile number\"\n            name=\"mobile\"\n            type=\"tel\"\n            autoComplete=\"mobile\"\n            autoFocus\n            onChange={handleInput}\n            value={props.location.mobile}\n            disabled={Boolean(props.location.mobile)}\n          />\n\n          <Button\n            fullWidth\n            disabled={disableSubmit}\n            variant=\"contained\"\n            color=\"primary\"\n            className={classes.submit}\n            onClick={handleSubmit}\n            id=\"f-captcha\"\n          >\n            Reset Password\n          </Button>\n          <Grid container>\n            <Grid item xs>\n              <Link to=\"/help\" variant=\"body2\">\n                Need Help.? Call Us on 6306683868\n              </Link>\n            </Grid>\n            <Grid item>\n              <Link to=\"/login\" variant=\"body2\">\n        